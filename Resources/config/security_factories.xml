<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="fos_user.security.authentication.factory.oauth.class">FOS\UserBundle\DependencyInjection\Security\Factory\OAuthFactory</parameter>
    </parameters>
    <services>
        <service id="fos_user.security.authentication.factory.oauth"
            class="%fos_user.security.authentication.factory.oauth.class%"
            public="false">
            <tag name="security.listener.factory" />
        </service>
    </services>
</container>